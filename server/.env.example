# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/connect-converse

# JWT Secret (Generate using: node scripts/generate-jwt-secret.js)
JWT_SECRET=your-super-secret-jwt-key-here

# Cloudinary Configuration
